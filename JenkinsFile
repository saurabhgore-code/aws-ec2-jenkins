pipeline {
	agent any
    environment {
		AWS_CREDENTIALS=credentials('aws_keys')
	}
    stages
	{
	stage('connect to cli')
	{ 
	        steps{ 
		   sh 'aws configure'
		   }
        }
	stage('start instance')
		{
			steps{	
	 sh 'aws ec2 start-instances --instance-ids i-01de8b5c6162d90c4'
			}
		}
	}
}
