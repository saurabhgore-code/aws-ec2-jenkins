pipeline{
    agent any
      stages{
          stage("Start instances"){
              steps{
                  withCredentials([[
                      $class: 'AmazonWebServicesCredentialsBinding',
                       credentialsId: 'aws-keys',
                      accessKeyVariable: 'AWS_ACCESS_KEY_ID',
                      secretKeyVariable: 'AWS_SECRET_ACCESS_KEY']]){
sh  'aws ec2 start-instances --instance-ids `aws ec2 describe-instances --filters "Name=tag:sagore,Values=control-machine" --query 'Reservations[].Instances[].InstanceId'
 --output text`'
                      }
              }
          }
      }
}
