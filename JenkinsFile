pipeline{
    agent any
      stages{
         stage ('SCM checkout')
       {
     steps {
      bat 'git clone https://github.com/saurabhgore-code/aws-jenkins.git'
  }
    }
          stage("Start instances"){
              steps
                {  withCredentials([[
                      $class: 'AmazonWebServicesCredentialsBinding',
                       credentialsId: 'aws-keys',
                     accessKeyVariable: 'AWS_ACCESS_KEY_ID',
                      secretKeyVariable: 'AWS_SECRET_ACCESS_KEY']]){ 
                  bat 'aws ec2 start-instances --instance-ids $InstanceId'
                    }
              }
}
      

              }
           
       /*stage("Stop instances"){
              steps{
        sh 'aws ec2 stop-instances --instance-ids $InstanceId'
        }
  
   }
*/

}
